{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "es2020", "esnext"],
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"module": "commonjs",
		"resolveJsonModule": true,
		"sourceMap": true,
		"declaration": true,
		"esModuleInterop": true,
		"jsx": "react"
	}
}
